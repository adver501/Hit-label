            </android.support.constraint.ConstraintLayout>
        </RelativeLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/RelativeL">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/topC"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="58dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ScrollView
                    android:id="@+id/ScrollVu"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/llinSV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" />
                </ScrollView>

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/percent_fab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fab_margin"
                    app:backgroundTint="#FFFFFF"
                    app:fabSize="mini"
                    app:layout_constraintBottom_toTopOf="@id/Tag_fab"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="1.0"
                    app:srcCompat="@drawable/percentage" />
                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/Tag_fab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fab_margin"
                    app:backgroundTint="#FFFFFF"
                    app:fabSize="mini"
                    app:layout_constraintBottom_toTopOf="@id/Load_fab"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="1.0"
                    app:srcCompat="@drawable/tack" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/Load_fab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fab_margin"
                    android:background="@color/white"
                    android:visibility="visible"
                    app:backgroundTint="@color/white"
                    app:fabSize="mini"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="1.0"
                    app:srcCompat="@drawable/download" />
            </android.support.constraint.ConstraintLayout>

            <android.support.design.widget.BottomNavigationView
                android:id="@+id/nv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_gravity="bottom"
                android:background="@color/colorPrimary"
                app:itemBackground="@color/colorPrimary"
                app:itemIconTint="@color/white"
                app:itemTextColor="@color/orange"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:menu="@menu/navigation"
                tools:layout_editor_absoluteY="325dp" />
        </android.support.constraint.ConstraintLayout>
    </RelativeLayout>

</android.support.design.widget.CoordinatorLayout>